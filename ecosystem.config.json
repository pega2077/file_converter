{
  "apps": [
    {
      "name": "file-converter-service",
      "script": "dist/index.js",
      "cwd": "./",
      "instances": 1,
      "exec_mode": "fork",
      "watch": false,
      "env": {
        "NODE_ENV": "development"
      },
      "env_production": {
        "NODE_ENV": "production"
      },
      "env_file": ".env",
      "error_file": "logs/pm2-error.log",
      "out_file": "logs/pm2-out.log",
      "merge_logs": true
    }
  ]
}
